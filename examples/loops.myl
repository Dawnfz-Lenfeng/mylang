// Simple counting loop
let i = 0;
while i < 5 {
    print "Count:", i;
    i += 1;
}

print "for loop";
for let x = 0; x < 3; x += 1 {
    for let y = 0; y < 3; y += 1 {
        print "Position:", x, y;
    }
}

// while loop is the same as for loop
print "while loop";
let x = 0;
while x < 3 {
    let y = 0;
    while y < 3 {
        print "Position:", x, y;
        y += 1;
    }
    x += 1;
}
