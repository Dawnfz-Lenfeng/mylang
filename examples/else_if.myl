// Testing nested if with else if
print "=== Testing nested if with else if ===";
let x = 15;
let y = 25;

if x > 10 {
    print "x is greater than 10";
    if y > 20 {
        print "y is also greater than 20";
        if x + y > 35 {
            print "sum is greater than 35";
        } else if x + y > 30 {
            print "sum is greater than 30";
        } else {
            print "sum is 30 or less";
        }
    } else if y > 15 {
        print "y is greater than 15 but not 20";
    } else {
        print "y is 15 or less";
    }
} else if x > 5 {
    print "x is greater than 5 but not 10";
} else {
    print "x is 5 or less";
}