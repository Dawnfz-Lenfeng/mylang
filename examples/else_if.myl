// Testing multiple else if with different values
let values = [45, 65, 75, 85, 95];
let i = 0;
while i < 5 {
    let val = values[i];
    print "Testing value:", val;
    
    if val >= 90 {
        print "  Excellent!";
    } else if val >= 80 {
        print "  Good!";
    } else if val >= 70 {
        print "  Average";
    } else if val >= 60 {
        print "  Below average";
    } else {
        print "  Poor";
    }
    
    i = i + 1;
}

