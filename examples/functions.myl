fn add(a, b) {
    return a + b;
}

fn multiply(x, y) {
    return x * y;
}

fn calculate_area(width, height) {
    return multiply(width, height);
}

fn main() {
    let areas = [
        [10, 20],
        [30, 40],
        [50, 60],
    ];

    for area in areas {
        let width = area[0];
        let height = area[1];
        let area = calculate_area(width, height);
        print("Area = ");
        print(area);
    }
}
